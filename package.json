{
  "name": "__project_name__",
  "license": "MIT",
  "scripts": {
    "bsb:make": "bsb -make-world",
    "bsb:watch": "bsb -make-world -w",
    "bsb:clean": "bsb -clean-world",
    "clean:dist": "rm -rf dist",
    "watch:bs": "yarn bsb:clean && yarn bsb:watch",
    "wait-bucklescript": "wait-on ./lib/js/src/App.js",
    "watch:js": "yarn wait-bucklescript && yarn clean:dist && webpack-dev-server",
    "build": "npm-run-all bsb:clean bsb:make clean:dist && webpack --mode=\"production\"",
    "start": "npm-run-all --parallel watch:bs watch:js"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "reason-react": "^0.5.3"
  },
  "devDependencies": {
    "bs-platform": "^4.0.6",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.23.1",
    "wait-on": "^3.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
